@using Microsoft.AspNetCore.Identity
@using CalifornianHealthMonolithic.Shared.Models.Entities
@inject SignInManager<Patient> SignInManager
@inject UserManager<Patient> UserManager

@if (SignInManager.IsSignedIn(User))
{
    <li class=""><a href="/Identity/Account/Logout">Log out</a></li>
}
else 
{
    <li class=""><a href="/Identity/Account/Login">Login</a></li>
    <li class=""><a href="/Identity/Account/Register">Register</a></li>
}