{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "dependsOrder": "sequence",
            "dependsOn": ["apiBookingBuild", "apiConsultantsBuild", "webAppBuild"]
        },
        {
            "label": "apiBookingBuild",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/CalifornianHealthMonolithic.APIBooking/CalifornianHealthMonolithic.APIBooking.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "apiConsultantsBuild",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/CalifornianHealthMonolithic.APIConsultant/CalifornianHealthMonolithic.APIConsultant.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "webAppBuild",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/CalifornianHealthMonolithic.WebApp/CalifornianHealthMonolithic.WebApp.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "sharedBuild",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/CalifornianHealthMonolithic.Shared/CalifornianHealthMonolithic.Shared.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "dataBuild",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/CalifornianHealthMonolithic.Data/CalifornianHealthMonolithic.Data.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}