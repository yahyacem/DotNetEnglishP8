{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (APIBooking)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/CalifornianHealthMonolithic.APIBooking/bin/Debug/net7.0/CalifornianHealthMonolithic.APIBooking.dll",
            "args": [],
            "cwd": "${workspaceFolder}/CalifornianHealthMonolithic.APIBooking",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            },
            "requireExactSource": false
        },
        {
            "name": ".NET Core Launch (APIConsultant)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/CalifornianHealthMonolithic.APIConsultant/bin/Debug/net7.0/CalifornianHealthMonolithic.APIConsultant.dll",
            "args": [],
            "cwd": "${workspaceFolder}/CalifornianHealthMonolithic.APIConsultant",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            },
            "requireExactSource": false
        },
        {
            "name": ".NET Core Launch (WebApp)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/CalifornianHealthMonolithic.WebApp/bin/Debug/net7.0/CalifornianHealthMonolithic.WebApp.dll",
            "args": [],
            "cwd": "${workspaceFolder}/CalifornianHealthMonolithic.WebApp",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/CalifornianHealthMonolithic.WebApp/Views"
            },
            "requireExactSource": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ],
    "compounds": [
        {
            "name": "Compound",
            "configurations": [".NET Core Launch (APIBooking)", ".NET Core Launch (APIConsultant)", ".NET Core Launch (WebApp)"]
        }
    ]
}